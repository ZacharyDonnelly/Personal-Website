{
  "name": "zachdonnelly.com",
  "version": "2.0.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7.25.0"
  },
  "description": "rebuild of zachdonnelly.com",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZacharyDonnelly/zachdonnelly.com.git"
  },
  "author": "Zach Donnelly (https://zachdonnelly.com/)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/ZacharyDonnelly/zachdonnelly.com/issues"
  },
  "homepage": "https://github.com/ZacharyDonnelly/zachdonnelly.com#readme",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "clean": "rm -rf ./node_modules && rm -rf ./.next",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint:eslint": "eslint --fix --cache --cache-strategy metadata --cache-location ./node_modules/.cache/eslint --ext .ts,.tsx src/",
    "lint:md": "./tools/check-md.sh && markdownlint --fix './**/*.md'",
    "lint:prettier": "prettier --write --cache --cache-strategy metadata \"**/*.{css,scss}\"",
    "lint:stylelint": "pnpm stylelint --config='./.stylelintrc.json' --fix --cache --cache-location ./node_modules/.cache/stylelint \"**/*.{css,scss,module.scss}\"",
    "lint:yaml": "prettier --write './**/*.{yaml,yml}'",
    "sass": "sass --watch app/:app/",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b . && tsc -p cypress",
    "validate": "./tools/validate.sh"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "bcryptjs": "^2.4.3",
    "framer-motion": "^9.0.4",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "react-intersection-observer": "^9.4.2",
    "remix-image": "^1.4.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@faker-js/faker": "^7.6.0",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@next/eslint-plugin-next": "^13.1.6",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.2",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "happy-dom": "^8.1.4",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "markdownlint-cli": "^0.33.0",
    "msw": "^0.49.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.9.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remark": "^14.0.2",
    "remark-cli": "^11.0.0",
    "remix": "^1.0.0",
    "sass": "^1.58.2",
    "scriptlint": "^2.2.0",
    "start-server-and-test": "^1.15.2",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.4.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.27.2"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "peerDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@types/react": "^17.0.53",
    "remix": "^1.0.0"
  }
}
